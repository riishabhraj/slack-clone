{
    "version": 2,
    "buildCommand": "pnpm run build",
    "installCommand": "pnpm install",
    "framework": "nextjs",
    "outputDirectory": ".next",
    "regions": [
        "iad1"
    ],
    "git": {
        "deploymentEnabled": {
            "main": true
        }
    },
    "env": {
        "PNPM_FLAGS": "--shamefully-hoist --unsafe-perm"
    },
    "build": {
        "env": {
            "NEXT_TELEMETRY_DISABLED": "1",
            "PNPM_ENABLE_PRE_POST_SCRIPTS": "true"
        }
    }
}